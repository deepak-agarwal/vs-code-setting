    "prettier.semi": false,
    "prettier.useTabs": true,
    "editor.fontSize": 15,
    "customizeUI.fontSizeMap": {
        "tab-title": "12px",
    },
    "customizeUI.font.regular": "Open Sans",
    "customizeUI.font.monospace": "OpenSans-Light",
    
        "emmet.includeLanguages": {
